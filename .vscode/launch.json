{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File with profiler",
      "type": "debugpy",
      "request": "launch",
      "module": "cProfile",
      "args": [
        "-o",
        "/tmp/tmp.prof",
        "${file}"
      ]
    },
    {
      "name": "Python: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    }
  ]
}
